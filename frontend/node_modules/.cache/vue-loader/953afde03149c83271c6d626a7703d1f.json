e=\"background-color: #001a33;\">\r\n\t\t\t\t<div class=\"p-4\">\r\n\t\t\t\t\t<h1><a href=\"\" class=\"logo\"><img src=\"../../../public/pages/img/sunlogo(original).png\" width=\"100%\" alt=\"\"></a></h1>\r\n        <div class=\"mb-5\">\r\n        <ul class=\"list-unstyled components mb-5\">\r\n          <li class=\"active\">\r\n            <a href=\"/manager/events\"><span class=\"fa fa-calendar mr-3\"></span> Events</a>\r\n          </li>\r\n          <li>\r\n            <a href=\"/manager/suppliers\"><span class=\"fa fa-handshake-o mr-3\"></span>Suppliers</a>\r\n          </li>\r\n          <li>\r\n            <a href=\"/manager/deliveries\"><span class=\"fa fa-truck mr-3\"></span> Deliveries</a>\r\n          </li>\r\n          <li>\r\n            <a href=\"/manager/employees\"><span class=\"fa fa-users mr-3\"></span> Employees</a>\r\n          </li>\r\n          <li>\r\n            <a href=\"/manager/inbox\"><span class=\"fa fa-inbox mr-3\"></span>  Inbox</a>\r\n          </li>\r\n        </ul>\r\n        </div>\r\n\t\t\t\t\t<div class=\"footer\">\r\n\t\t\t\t\t\t<ul class=\"list-unstyled components mb-5\">\r\n\t\t\t\t\t\t\t<li>\r\n\t\t\t\t\t\t\t\t<a href=\"/\"><span class=\"fa fa-sign-out mr-3\"></span> Logout</a>\r\n\t\t\t\t\t\t\t</li>\r\n\t\t\t\t\t\t</ul>\r\n\t\t\t\t\t</div>\r\n\r\n\t\t\t\t</div>\r\n\t\t\t</nav>\r\n\r\n\t\t\t\t<!-- Page Content  -->\r\n\t\t\t    <div id=\"content\" class=\"p-4 p-md-5 pt-5\">\r\n      <div>\r\n        <h1>Events</h1>\r\n        <br>\r\n        <div class=\"row\">\r\n          <div class=\"col-sm-3\">\r\n            <h3>Your Events </h3>\r\n            <div>\r\n              <a class=\"list-group-item list-group-item-action\" v-for=\"event in events\" :key=\"event.id\" @click=\"showEvent(event.id)\">\r\n                {{event.event_name}}\r\n              </a>\r\n            </div>\r\n            <div class=\"mt-5 mx-auto\">\r\n              <a href=\"/client/reservation\">\r\n                <button class=\"btn btn-success\">Reserve an Event</button>\r\n              </a>\r\n            </div> \r\n          </div>\r\n          <div class=\"col\">\r\n            <h3>Event Details</h3>\r\n            <div>\r\n            Project Status: {{eventStatus}}\r\n            <br>\r\n            Current Task:\r\n            <br>\r\n            Team Manager:\r\n            </div> \r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: 'ManagerEvents',\r\n  data() {\r\n    return {\r\n      eventStatus: \"\",\r\n      events: [],\r\n      services: [],\r\n    };\r\n  },\r\n\r\n  created() {\r\n    this.getEvents();\r\n    this.getServices();\r\n  },\r\n\r\n  methods: {\r\n    async getServices() {\r\n      try {\r\n        const response = await axios.get(\"http://localhost:3000/services\");\r\n        this.services = response.data;\r\n        console.log(this.services);\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    async getEvents() {\r\n      try {\r\n        const response = await axios.get(\"http://localhost:3000/event\");\r\n        this.events = response.data;\r\n        console.log(this.events);\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    },\r\n    async showEvent(id){\r\n      try {\r\n        const response = await axios.get(`http://localhost:3000/event/${id}`)\r\n        console.log(response);\r\n        this.eventName = response.data.eventName;\r\n        this.eventStatus = response.data.status;\r\n      } catch (err) {\r\n        console.log(err);\r\n      }\r\n    }\r\n  },\r\n  },\r\n};\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n</style>\r\n"]}]}